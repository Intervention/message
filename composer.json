{
    "name": "intervention/message",
    "description": "Quick flash message setup for Laravel",
    "homepage": "http://intervention.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@olivervogel.net",
            "homepage": "http://olivervogel.net/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4|~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\Message\\": "src/Intervention/Message"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Message\\MessageServiceProvider"
            ],
            "aliases": {
                "Message": "Intervention\\Message\\Facades\\Message"
            }
        }
    },
    "minimum-stability": "stable"
}
